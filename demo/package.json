{
  "name": "tasit-demo",
  "version": "0.0.1",
  "description": "A demo app showing proper usage of the Tasit SDK",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "eject": "npx expo eject",
    "test": "npx jest",
    "lint": "npx prettier --write '{*.js,**/*.js,*.jsx,**/*.jsx}' && npx eslint '{*.js,**/*.js,*.jsx,**/*.jsx,**/*.snap}'"
  },
  "dependencies": {
    "expo": "^32.0.0",
    "prop-types": "^15.6.2",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-responsive-dimensions": "^2.0.1",
    "react-navigation": "^3.0.9",
    "tasit-sdk": "0.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "expo-cli": "^2.4.0",
    "jest": "^23.6.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.15.2",
    "react-dom": "^16.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasitlabs/tasit.git"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "\\.js$": "<rootDir>/jest.preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|.*react-navigation|expo-react-native-adapter)"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "keywords": [
    "dapp",
    "ethereum",
    "react",
    "native"
  ],
  "author": "Tasit Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tasitlabs/tasit/issues"
  },
  "homepage": "https://tasit.io"
}
